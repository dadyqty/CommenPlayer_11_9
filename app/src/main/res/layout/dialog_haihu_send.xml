<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="6.0dp"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="经度增量："
            android:textColor="#000000"
            android:textSize="20sp" />
    <EditText
        android:id="@+id/jingdudu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="度"
        android:maxLength="9"
        android:textAlignment="center"
        android:background="@null"
        android:text="59"
        android:gravity="center_horizontal" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="°"
            android:textColor="#000000"
            android:textSize="20sp" />
    <EditText
        android:id="@+id/jingdufen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="分"
        android:maxLength="9"
        android:background="@null"
        android:text="59" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="'"
            android:textColor="#000000"
            android:textSize="20sp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5.0dip"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="纬度增量："
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/weidudu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="度"
            android:maxLength="9"
            android:textAlignment="center"
            android:background="@null"
            android:text="59"
            android:gravity="center_horizontal" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="°"
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/weidufen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="分"
            android:maxLength="9"
            android:background="@null"
            android:text="59" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="'"
            android:textColor="#000000"
            android:textSize="20sp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5.0dip"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="象限："
            android:textColor="#000000"
            android:textSize="20sp" />
    <EditText
        android:id="@+id/xiangxian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint=""
        android:maxLength="9"
        android:background="@null"
        android:text="3" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5.0dip"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="参考点经度："
            android:textColor="#000000"
            android:textSize="20sp" />
    <EditText
        android:id="@+id/cankaojingdudu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="度"
        android:maxLength="9"
        android:background="@null"
        android:text="139" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="°"
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/cankaojingdufen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="分"
            android:maxLength="9"
            android:background="@null"
            android:text="35" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="'"
            android:textColor="#000000"
            android:textSize="20sp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5.0dip"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="参考点纬度："
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/cankaoweidudu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="度"
            android:maxLength="9"
            android:background="@null"
            android:text="12" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="°"
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/cankaoweidufen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="分"
            android:maxLength="9"
            android:background="@null"
            android:text="15" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="'"
            android:textColor="#000000"
            android:textSize="20sp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5.0dip"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="信道号："
            android:textColor="#000000"
            android:textSize="20sp" />
    <EditText
        android:id="@+id/xindaohao7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint=""
        android:maxLength="9"
        android:background="@null"
        android:text="100" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:text="船位："
            android:textColor="#000000"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/jindu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textSize="20dp"
            android:text=""/>
    </LinearLayout>
    <TextView
        android:id="@+id/weidu"
        android:layout_marginLeft="60dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:layout_below="@id/jindu"
        android:text=""/>
    <TextView
        android:id="@+id/haihuhujiao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_below="@id/weidu"
        android:gravity="center"
        android:textSize="25dp"
        android:text=""/>

</LinearLayout>
