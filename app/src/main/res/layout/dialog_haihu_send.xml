<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5.0dip"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="参考点经度："
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/cankaojingdudu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="度"
            android:maxLength="3"
            android:background="@null"
            android:text="139" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="°"
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/cankaojingdufen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="分"
            android:maxLength="2"
            android:background="@null"
            android:text="35" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="'"
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/cankaojingdumiao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="秒"
            android:maxLength="2"
            android:background="@null"
            android:text="35" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="''"
            android:textColor="#000000"
            android:textSize="20sp" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text= " E"
            android:textColor="#000000"
            android:textSize="20sp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5.0dip"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="参考点纬度："
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/cankaoweidudu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="度"
            android:maxLength="3"
            android:background="@null"
            android:textSize="20sp"
            android:text="12" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="°"
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/cankaoweidufen"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="分"
            android:maxLength="2"
            android:background="@null"
            android:textSize="20sp"
            android:text="15" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="'"
            android:textColor="#000000"
            android:textSize="20sp" />
        <EditText
            android:id="@+id/cankaoweidumiao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="秒"
            android:maxLength="2"
            android:textSize="20sp"
            android:background="@null"
            android:text="15" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="'' N"
            android:textColor="#000000"
            android:textSize="20sp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#000000" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="6.0dp"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="偏移量："
            android:textColor="#000000"
            android:textSize="20sp" />
    <EditText
        android:id="@+id/jingdufen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="分"
        android:maxLength="2"
        android:textAlignment="center"
        android:background="@null"
        android:text="59"
        android:textSize="20sp"
        android:gravity="center_horizontal" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="' E"
            android:textColor="#000000"
            android:textSize="20sp" />
    <EditText
        android:id="@+id/weidufen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10sp"
        android:hint="秒"
        android:maxLength="2"
        android:background="@null"
        android:textSize="20sp"
        android:text="59" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="' S"
            android:textColor="#000000"
            android:textSize="20sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/haihuhujiao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/weidu"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text=""
        android:textSize="25dp" />



</LinearLayout>
